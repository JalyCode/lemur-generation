<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.afterturn.gen.modular.code.dao.TableFieldInfoDao">

    <resultMap id="BaseResultMap" type="cn.afterturn.gen.modular.code.model.TableFieldInfoModel">
        <result column="id" property="id"/>
        <result column="content" property="content"/>
        <result column="dict_field" property="dictField"/>
        <result column="dict_table" property="dictTable"/>
        <result column="dict_text" property="dictText"/>
        <result column="field_default" property="fieldDefault"/>
        <result column="field_href" property="fieldHref"/>
        <result column="field_length" property="fieldLength"/>
        <result column="field_name" property="fieldName"/>
        <result column="field_valid_type" property="fieldValidType"/>
        <result column="is_key" property="isKey"/>
        <result column="is_null" property="isNull"/>
        <result column="is_query" property="isQuery"/>
        <result column="is_show" property="isShow"/>
        <result column="is_show_list" property="isShowList"/>
        <result column="length" property="length"/>
        <result column="order_num" property="orderNum"/>
        <result column="point_length" property="pointLength"/>
        <result column="query_mode" property="queryMode"/>
        <result column="show_type" property="showType"/>
        <result column="type" property="type"/>
        <result column="update_by" property="updateBy"/>
        <result column="update_date" property="updateDate"/>
        <result column="update_name" property="updateName"/>
        <result column="table_id" property="tableId"/>
    </resultMap>


    <select id="selectCount" resultMap="BaseResultMap">
        select count(1) from T_SYSTEM_TABLE_FIELD t
        <where>
            <include refid="select_content"/>
        </where>
    </select>

    <select id="selectList" resultMap="BaseResultMap">
        select * from T_SYSTEM_TABLE_FIELD t
        <where>
            <include refid="select_content"/>
        </where>
    </select>

    <select id="selectPage" resultMap="BaseResultMap">
        select * from T_SYSTEM_TABLE_FIELD t
        <where>
            <include refid="select_content"/>
            ${w.sqlSegment}
        </where>
    </select>

    <sql id="select_content">
        <if test="e.id != null and e.id != '' ">
            AND t.ID = #{e.id}
        </if>
        <if test="e.content != null and e.content != '' ">
            AND t.CONTENT = #{e.content}
        </if>
        <if test="e.dictField != null and e.dictField != '' ">
            AND t.DICT_FIELD = #{e.dictField}
        </if>
        <if test="e.dictTable != null and e.dictTable != '' ">
            AND t.DICT_TABLE = #{e.dictTable}
        </if>
        <if test="e.dictText != null and e.dictText != '' ">
            AND t.DICT_TEXT = #{e.dictText}
        </if>
        <if test="e.fieldDefault != null and e.fieldDefault != '' ">
            AND t.FIELD_DEFAULT = #{e.fieldDefault}
        </if>
        <if test="e.fieldHref != null and e.fieldHref != '' ">
            AND t.FIELD_HREF = #{e.fieldHref}
        </if>
        <if test="e.fieldLength != null and e.fieldLength != '' ">
            AND t.FIELD_LENGTH = #{e.fieldLength}
        </if>
        <if test="e.fieldName != null and e.fieldName != '' ">
            AND t.FIELD_NAME = #{e.fieldName}
        </if>
        <if test="e.fieldValidType != null and e.fieldValidType != '' ">
            AND t.FIELD_VALID_TYPE = #{e.fieldValidType}
        </if>
        <if test="e.isKey != null and e.isKey != '' ">
            AND t.IS_KEY = #{e.isKey}
        </if>
        <if test="e.isNull != null and e.isNull != '' ">
            AND t.IS_NULL = #{e.isNull}
        </if>
        <if test="e.isQuery != null and e.isQuery != '' ">
            AND t.IS_QUERY = #{e.isQuery}
        </if>
        <if test="e.isShow != null and e.isShow != '' ">
            AND t.IS_SHOW = #{e.isShow}
        </if>
        <if test="e.isShowList != null and e.isShowList != '' ">
            AND t.IS_SHOW_LIST = #{e.isShowList}
        </if>
        <if test="e.length != null and e.length != '' ">
            AND t.LENGTH = #{e.length}
        </if>
        <if test="e.orderNum != null and e.orderNum != '' ">
            AND t.ORDER_NUM = #{e.orderNum}
        </if>
        <if test="e.pointLength != null and e.pointLength != '' ">
            AND t.POINT_LENGTH = #{e.pointLength}
        </if>
        <if test="e.queryMode != null and e.queryMode != '' ">
            AND t.QUERY_MODE = #{e.queryMode}
        </if>
        <if test="e.showType != null and e.showType != '' ">
            AND t.SHOW_TYPE = #{e.showType}
        </if>
        <if test="e.type != null and e.type != '' ">
            AND t.TYPE = #{e.type}
        </if>
        <if test="e.updateBy != null and e.updateBy != '' ">
            AND t.UPDATE_BY = #{e.updateBy}
        </if>
        <if test="e.updateDate != null and e.updateDate != '' ">
            AND t.UPDATE_DATE = #{e.updateDate}
        </if>
        <if test="e.updateName != null and e.updateName != '' ">
            AND t.UPDATE_NAME = #{e.updateName}
        </if>
        <if test="e.tableId != null and e.tableId != '' ">
            AND t.TABLE_ID = #{e.tableId}
        </if>
    </sql>

</mapper>